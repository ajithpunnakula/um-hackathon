{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "methods": ["GET"],
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "api/index.py"
    },
    {
      "src": "/api/url",
      "methods": ["POST"],
      "dest": "api/index.py"
    },
    {
      "src": "/api/transcript",
      "methods": ["POST"],
      "dest": "api/index.py"
    }
  ]
}